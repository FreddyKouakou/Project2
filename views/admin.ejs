
<table id="accountsTable">
<tr>
  <th>Name</th>
  <th class>username</th>
  <th class="optional">Phone Number</th>
  <th class="optional">Account Type</th>
<tr>
<% for (let i = 0; i < users.length; i++) { %>

<tr>
  <td><%=users[i].first_name %> <%=users[i].last_name %></td>
  <td><%=users[i].username %></td>
  <td class="optional"><%=users[i].phone_number %></td>
  <td class="optional"><%=users[i].account_type %></td>
</tr>
<% } %>
</table>

<fieldset>
    <legend>Create User Account</legend>
        <!-- Will be changed to POST soon -->
        <h5 id="signUpData" class="errMessage"></h5>
        <h5>First Name* <input id="firstName" required name="first_name" type="text"></h5>
        <h5>Last Name* <input id="lastName" required name="last_name" type="text"></h5>
        <h5>User Name* <input id="username" required name="user_name" type="text"></h5>
        <h5>Phone Number* <input id="phoneNumber" required name="phone_number" type="tel"></h5>
        <h5>Password* <input id="password" required name="pass_word" type="password" oninput="confirmPassword();"></h5>
        <h5>Confirm Password* <input id="confirmPassword" type="password" oninput="confirmPassword();"><span id="passwordMatch" class="error">Passwords must match</span></span></h5>
        <h5>User Type*
        <select id="accountType">
          <option value="student">Student</option>
          <option value="admin">Admin</option>
        </select>
        </h5>
        <button id="submitBtn" class="invisible" onclick="createAccount()">Create Account</button>
</fieldset>